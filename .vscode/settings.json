{
  "deno.enable": true,
  "deno.lint": true,
  "deno.suggest.imports.hosts": {
    "https://deno.land": false
  },
  "[typescript]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "editor.tabSize": 2,
  "workbench.colorCustomizations": {
    "activityBar.background": "#1A331A",
    "titleBar.activeBackground": "#244825",
    "titleBar.activeForeground": "#F8FCF8"
  }
}
